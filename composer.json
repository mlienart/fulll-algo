{
    "autoload": {
        "psr-4": {
          "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "scripts": {
        "fix": "PHP_CS_FIXER_IGNORE_ENV=1 php-cs-fixer fix --config=.php-cs-fixer.dist.php"
    }
}
